{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": [
  true,
  {
    "import-sources-order": "any",
    "named-imports-order": "any"
  }
],
    "origin-ordered-imports": false,
    "no-console": 0
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
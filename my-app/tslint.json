{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "ordered-imports": [
  true,
  {
    "import-sources-order": "lowercase-last",
    "named-imports-order": "lowercase-first"
  }
]
}
